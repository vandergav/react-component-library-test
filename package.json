{
  "name": "vandergav-react-component-library-test",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "styled-components": "^5.3.1"
  },
  "scripts": {
    "compile-icons": "npx @svgr/cli -d src/icons src/icons --typescript",
    "copy-fonts": "copyfiles -u 1 src/config/fonts/* lib/",
    "clean:mac": "rm -rf ./lib",
    "clean": "rimraf ./lib",
    "prebuild": "npm run clean && npm run compile-icons",
    "build": "tsc --build \"./tsconfig.json\"",
    "postbuild": "npm run copy-fonts",
    "prepublish": "npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:storybook": "start-storybook -p 6006 -s public",
    "build:storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/node-logger": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@svgr/cli": "^5.5.0",
    "@types/react": "^17.0.21",
    "@types/styled-components": "^5.1.14",
    "copyfiles": "^2.4.1",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  }
}
